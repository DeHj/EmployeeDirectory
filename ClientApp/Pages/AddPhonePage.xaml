<StackPanel x:Class="ClientApp.Pages.AddPhonePage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:res="clr-namespace:ClientApp.Properties"
             xmlns:e="clr-namespace:ClientApp.Elements"
             xmlns:local="clr-namespace:ClientApp.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <TextBlock DockPanel.Dock="Top" Text="{x:Static res:Resources.addPhoneNumber_TextBlock1}"/>
    <TextBlock x:Name="login_TextBlock" DockPanel.Dock="Top" Text="employee_login"/>
    <e:PhoneNumberBox x:Name="phoneNumberBox"/>
    <Button x:Name="addPhone" Content="{x:Static res:Resources.addPhoneNumber_Button1}" Click="addPhone_Click"/>
</StackPanel>
