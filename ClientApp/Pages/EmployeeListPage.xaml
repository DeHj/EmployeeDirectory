<DockPanel x:Class="ClientApp.Pages.EmployeeListPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ClientApp.Pages"
      mc:Ignorable="d"
      Width="300"
      Height="400">
    <Border DockPanel.Dock="Top" BorderThickness="1">
        <StackPanel Margin="10 10 10 10">
            <TextBox Name="firstName"></TextBox>
            <TextBox Name="secondName"></TextBox>
            <TextBox Name="middleName"></TextBox>
            <Button DockPanel.Dock="Right" Click="Find_Click" HorizontalAlignment="Left">Find</Button>
        </StackPanel>
    </Border>

    <DockPanel DockPanel.Dock="Bottom" HorizontalAlignment="Center">
        <Button x:Name="PrevPage" HorizontalAlignment="Left" Width="20">&lt;</Button>
        <Button x:Name="NextPage" HorizontalAlignment="Right" Width="20">&gt;</Button>
    </DockPanel>
    
    <StackPanel x:Name="Employees">
        
    </StackPanel>
</DockPanel>
